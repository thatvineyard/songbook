    "@types/express": "^4.16.1",
    "@types/node": "^11.11.7",
    "axios": "^0.18.0",
    "colors": "^1.3.3",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.2"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
